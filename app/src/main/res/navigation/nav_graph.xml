<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.xandealm.iwillpay.ui.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start">
    </fragment>

    <fragment
        android:id="@+id/expenseFragment"
        android:name="com.xandealm.iwillpay.ui.ExpenseFragment"
        android:label="Expense Edit"
        tools:layout="@layout/fragment_expense">
        <argument
            android:name="expenseId"
            app:argType="long" />
    </fragment>
    <action
        android:id="@+id/action_global_startFragment"
        app:destination="@id/startFragment" />
    <action
        android:id="@+id/action_global_expenseFragment"
        app:destination="@id/expenseFragment" />

</navigation>